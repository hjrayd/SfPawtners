{% extends 'base.html.twig' %}

{% block title %}Liste des likes{% endblock %}

{% block body %}
    <h1>Liste de vos likes</h1>
        {% for userLikes in userLikes %}

        <div class="cat-like">
            {{ userLikes.catOne.name }}
            {{ userLikes.likeDate}}
            <a href="{{ path('delete_like', {'id': userLikes.id}) }}" 
            class="btn btn-danger" 
            >Supprimer le like</a>
        </div>

        {% endfor %}
{% endblock %}
